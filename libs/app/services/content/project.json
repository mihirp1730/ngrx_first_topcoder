{
  "projectType": "library",
  "root": "libs/app/services/content",
  "sourceRoot": "libs/app/services/content/src",
  "prefix": "apollo",
  "targets": {
    "test": {
      "executor": "@nrwl/jest:jest",
      "outputs": ["coverage/libs/app/services/content"],
      "options": {
        "jestConfig": "libs/app/services/content/jest.config.js",
        "passWithNoTests": true
      }
    },
    "lint": {
      "executor": "@nrwl/linter:eslint",
      "options": {
        "lintFilePatterns": ["libs/app/services/content/src/**/*.ts", "libs/app/services/content/src/**/*.html"]
      },
      "outputs": ["{options.outputFile}"]
    }
  },
  "tags": []
}
